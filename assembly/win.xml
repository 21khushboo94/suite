<assembly>
  <id>raw</id>
  <formats>
    <format>zip</format>
  </formats>
  <includeBaseDirectory>false</includeBaseDirectory>
  <fileSets>
    <fileSet>
      <directory>geoserver/web/app/target/geoserver</directory>
      <outputDirectory>geoserver</outputDirectory>
      <excludes>
        <exclude>META-INF/**</exclude>
        <exclude>data/**</exclude>
      </excludes>
    </fileSet>
    <fileSet>
      <directory>geoserver/extension/target/dependency</directory>
      <outputDirectory>extension/oracle</outputDirectory>
      <includes>
        <include>gt-jdbc-oracle-*</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>geoserver/extension/target/dependency</directory>
      <outputDirectory>extension/arcsde</outputDirectory>
      <includes>
        <include>gt-arcsde-*</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>styler/target/styler</directory>
      <outputDirectory>styler</outputDirectory>
    </fileSet>
    <fileSet>
      <directory>geoexplorer/build/geoexplorer</directory>
      <outputDirectory>geoexplorer</outputDirectory>
    </fileSet>
    <fileSet>
      <outputDirectory>dashboard</outputDirectory>
      <includes>
        <include>assembly/dummy</include>
      </includes>
    </fileSet>
    <fileSet>
      <directory>docs/install/target/html</directory>
      <outputDirectory>docs/install</outputDirectory>
    </fileSet>
    <fileSet>
      <directory>docs/gettingstarted/target/html</directory>
      <outputDirectory>docs/gettingstarted</outputDirectory>
    </fileSet>
    <fileSet>
      <directory>docs/styler/target/html</directory>
      <outputDirectory>docs/styler</outputDirectory>
    </fileSet>
    <fileSet>
      <directory>docs/geoexplorer/target/html</directory>
      <outputDirectory>docs/geoexplorer</outputDirectory>
    </fileSet>
    <fileSet>
      <directory>docs/geoserver/target/html</directory>
      <outputDirectory>docs/geoserver</outputDirectory>
    </fileSet>
    <fileSet>
      <directory>recipes</directory>
      <outputDirectory>recipes</outputDirectory>
    </fileSet>
    <fileSet>
      <directory>data_dir</directory>
      <outputDirectory>data_dir</outputDirectory>
    </fileSet>
    <fileSet>
      <directory>jetty</directory>
      <outputDirectory>jetty</outputDirectory>
      <excludes>
        <exclude>pom.xml</exclude>
        <exclude>target/**</exclude>
      </excludes>
    </fileSet>
    <fileSet>
      <directory>assembly/jre</directory>
      <outputDirectory>jre</outputDirectory>
    </fileSet>
    <fileSet>
      <directory>assembly/gdal</directory>
      <outputDirectory>gdal</outputDirectory>
    </fileSet>
  </fileSets>
</assembly>
