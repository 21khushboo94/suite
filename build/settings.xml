<settings>
 <!-- uncomment this and replace @PATH@ to set up a custom maven repository -->
 <!--localRepository>@PATH@</localRepository-->
 <profiles>
  <profile>
   <id>no-snapshots</id>
   <repositories>
    <repository>
     <id>opengeo</id>
     <name>opengeo</name>
     <snapshots>
      <enabled>false</enabled>
      <updatePolicy>never</updatePolicy>
     </snapshots>
     <url>http://repo.opengeo.org/</url>
    </repository>
    <repository>
     <id>osgeo</id>
     <name>Open Source Geospatial Foundation Repository</name>
     <url>http://download.osgeo.org/webdav/geotools/</url>
     <snapshots>
      <enabled>false</enabled>
      <updatePolicy>never</updatePolicy>
     </snapshots>
    </repository>
    <repository>
     <id>org.mapfish</id>
     <name>MapFish Repository</name>
     <url>http://dev.mapfish.org/maven/repository</url>
     <snapshots>
      <enabled>false</enabled>
      <updatePolicy>never</updatePolicy>
     </snapshots>
    </repository>
   </repositories>
  </profile>
 </profiles>
 <activeProfiles>
  <activeProfile>no-snapshots</activeProfile>
 </activeProfiles>
</settings>
