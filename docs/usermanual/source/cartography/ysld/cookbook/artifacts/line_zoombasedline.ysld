name: Default Styler
title: 'YSLD Cook Book: Zoom-based line'
feature-styles:
- name: name
  rules:
  - name: Large
    scale: (,1.8E8)
    symbolizers:
    - line:
        stroke-color: '009933'
        stroke-width: 6
  - name: Medium
    scale: (1.8E8,3.6E8)
    symbolizers:
    - line:
        stroke-color: '009933'
        stroke-width: 4
  - name: Small
    scale: (3.6E8,)
    symbolizers:
    - line:
        stroke-color: '009933'
        stroke-width: 2
