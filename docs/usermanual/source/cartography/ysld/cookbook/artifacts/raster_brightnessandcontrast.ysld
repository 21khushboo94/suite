name: Default Styler
title: 'YSLD Cook Book: Brightness and contrast'
feature-styles:
- name: name
  rules:
  - symbolizers:
    - raster:
        opacity: 1.0
        color-map:
          type: ramp
          entries:
          - (008000,,70,)
          - (663333,,256,)
        contrast-enhancement:
          mode: normalize
          gamma: 0.5
