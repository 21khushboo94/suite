<?xml version="1.0" encoding="utf-8"?>
<gwcConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
                  xsi:noNamespaceSchemaLocation="http://geowebcache.org/schema/1.12.0/geowebcache.xsd" 
                  xmlns="http://geowebcache.org/schema/1.12.0">
<version>1.12.0</version>
<backendTimeout>120</backendTimeout>
<layers>

  <wmsLayer>
    <name>opengeo:countries</name>
    <gridSubsets> <!-- Use two of the predefined gridsets -->
      <gridSubset>
        <gridSetName>EPSG:4326</gridSetName>
      </gridSubset>
      <gridSubset>
        <gridSetName>EPSG:900913</gridSetName>
      </gridSubset>
    </gridSubsets>
    <mimeFormats><string>image/png</string></mimeFormats>
    <wmsUrl><string>http://demo.boundlessgeo.com/geoserver/wms</string></wmsUrl>
  </wmsLayer>

</layers>
</gwcConfiguration>
